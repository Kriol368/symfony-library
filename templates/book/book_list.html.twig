{% extends 'book/base.html.twig' %}
{% block title %} Books {% endblock %}
{% block body %}
    <h1>Book list</h1>
    {% if books %}
        {% for book in books %}
            {{ include ('book/partials/book_data.html.twig', { 'book': book }) }}
        {% endfor %}
    {% else %}
        <p>No books found</p>
    {% endif %}
{% endblock %}




